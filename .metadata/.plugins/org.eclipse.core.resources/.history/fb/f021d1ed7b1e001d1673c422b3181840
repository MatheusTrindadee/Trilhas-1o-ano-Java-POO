import javax.swing.JOptionPane;

public class Operacoes {

	public double calculaSoma(double num1, double num2) {
		double soma = num1 + num2;
		return soma;
	}

	public double calculaSub(double num1, double num2) {
		double sub = num1 - num2;
		return sub;
	}

	public double calculaMult(double num1, double num2) {
		double mult = num1 * num2;
		return mult;
	}

	public double calculaDiv(double num1, double num2) {
		double div = num1 / num2;
		return div;
	}

}
