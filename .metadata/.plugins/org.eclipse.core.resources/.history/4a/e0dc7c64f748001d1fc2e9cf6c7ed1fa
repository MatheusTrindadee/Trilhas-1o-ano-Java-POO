package modelo;

import java.util.ArrayList;

public class Carro {

	private String modelo;
	private String cor;
	ArrayList<Fabricados> listaDeCarros = new ArrayList<Fabricados>();

	public String getModelo() {
		return modelo;
	}

	public void setModelo(String modelo) {
		this.modelo = modelo;
	}

	public String getCor() {
		return cor;
	}

	public void setCor(String cor) {
		this.cor = cor;
	}

}
